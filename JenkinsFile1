currentBuild.displayName = "TEST BUILD"+currentBuild.number
pipeline {
    agent any
    environment {
        def var1="i am a variable"
        def var2="hsawstest"
        properties([pipelineTriggers([githubPush()])])
    }
    stages {
        stage ('Hello') {
            steps {
                echo 'Hello World'
                }
             }
        stage ('Set Environment') {
            steps {
                echo "${var1}"
                echo "${var2}"
                }
            }
    }
}
